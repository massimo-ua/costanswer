    "@bower_components/angular-mocks": "angular/bower-angular-mocks#*",
    "@bower_components/angular-resource": "angular/bower-angular-resource#*",
    "@bower_components/angular-sanitize": "angular/bower-angular-sanitize#*",
    "@bower_components/angular-toastr": "Foxandxss/angular-toastr#^2.0.0",
    "@bower_components/angular-ui-grid": "angular-ui/bower-ui-grid#^3.2.1",
    "@bower_components/angular-ui-router": "angular-ui/angular-ui-router-bower#*",
    "@bower_components/angular-ui-tinymce": "angular-ui/ui-tinymce#^0.0.16",
    "@bower_components/api-check": "kentcdodds/apiCheck.js#^7.5.5",
    "@bower_components/bootstrap": "twbs/bootstrap#^3.3.7",
